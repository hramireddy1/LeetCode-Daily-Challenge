def nthUglyNumber(self, n: int) -> int:
        a = b = c = 0
        ugly = [1]
        while len(ugly) < n:
            temp = min(ugly[a]*2, ugly[b]*3, ugly[c]*5)
            ugly.append(temp)
            if temp == ugly[a]*2: a+=1
            if temp == ugly[b]*3: b+=1
            if temp == ugly[c]*5: c+=1
        return ugly[-1]